{
  "name": "timeseries-sqlite2leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/worker.mjs",
  "module": "src/worker.mjs",
  "description": "migrate time series data from sqlite to leveldb",
  "keywords": [
    "time series"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "timeseries-sqlite2leveldb": "bin/timeseries-sqlite2leveldb"
  },
  "scripts": {
    "prepare": "rollup -c",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/worker.mjs --section=API",
    "lint": "documentation lint src/worker.mjs"
  },
  "dependencies": {
    "konsum-db": "^1.5.11",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^3.7.1",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.0",
    "documentation": "^12.3.0",
    "npm-pkgbuild": "^6.11.16",
    "rollup": "^2.7.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.3",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=12.16.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/timeseries-sqlite2leveldb#readme",
  "pacman": {
    "hooks": "pacman/hooks.sh",
    "installdir": "/services/timeseries-sqlite2leveldb/frontend",
    "content": {
      "/etc/nginx/config.d/${name}.conf": "pacman/nginx.conf"
    },
    "depends": {
      "nginx-mainline": ">=1.17.10",
      "systemd": ">=245.5"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-cli-app",
      "arlac77/template-esm-only",
      "arlac77/template-nginx",
      "arlac77/template-pacman",
      "arlac77/template-systemd"
    ]
  }
}
