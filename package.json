{
  "name": "timeseries-sqlite2leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "src/worker.js",
  "description": "migrate time series data from sqlite to leveldb",
  "keywords": [
    "time series"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "timeseries-sqlite2leveldb": "bin/timeseries-sqlite2leveldb"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/worker.js --section=API",
    "semantic-release": "semantic-release",
    "lint": "documentation lint src/worker.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "leveldown": "^4.0.0",
    "sqlite": "^3.0.0",
    "levelup": "^3.1.1"
  },
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "documentation": "^9.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.4",
    "semantic-release": "^15.12.5",
    "travis-deploy-once": "^5.0.9",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-json": "^3.1.0",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/timeseries-sqlite2leveldb#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
