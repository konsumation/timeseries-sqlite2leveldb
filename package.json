{
  "name": "timeseries-sqlite2leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/worker.mjs",
  "module": "src/worker.mjs",
  "description": "migrate time series data from sqlite to leveldb",
  "keywords": [
    "time series"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "timeseries-sqlite2leveldb": "bin/timeseries-sqlite2leveldb"
  },
  "scripts": {
    "prepare": "rollup -c",
    "test": "ava --node-arguments '--experimental-modules' --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --node-arguments '--experimental-modules' --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/worker.mjs --section=API",
    "lint": "documentation lint src/worker.mjs"
  },
  "dependencies": {
    "konsum-db": "^1.5.9",
    "leveldown": "^5.5.0",
    "levelup": "^4.3.1",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^3.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "rollup": "^1.31.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.2",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/timeseries-sqlite2leveldb#readme",
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
