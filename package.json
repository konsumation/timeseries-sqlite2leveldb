{
  "name": "timeseries-sqlite2leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "src/worker.js",
  "description": "migrate time series data from sqlite to leveldb",
  "keywords": [
    "time series"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/worker.js --section=API",
    "semantic-release": "semantic-release",
    "lint": "documentation lint src/worker.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "leveldown": "^4.0.0",
    "levelup": "^3.0.1",
    "sqlite": "^2.9.1"
  },
  "devDependencies": {
    "markdown-doctest": "^0.9.1",
    "nyc": "^12.0.2",
    "ava": "^0.25.0",
    "rollup": "^0.59.4",
    "semantic-release": "^15.5.0",
    "documentation": "^7.1.0",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": ">=10.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/timeseries-sqlite2leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/timeseries-sqlite2leveldb#readme",
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "presets": [],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
